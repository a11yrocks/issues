---
layout: nil
---
[
  {% for post in site.posts %}
  {% assign site_url = "https://issues.a11y.rocks" %}
    {
      "title": "{{ post.title }}",
      "url": "{{ site_url }}{{ post.url }}",
      "date": "{{ post.date }}",
      "app_name": "{{ post.app_name }}",
      "app_url": "{{ post.app_url }}",
      "title": "{{ post.title }}",
      "tag": "{{ post.tag }}",
      "media": "{{ site_url }}{{ post.media }}"
    }{% if forloop.last %}{% else %},{% endif %}
  {% endfor %}
]
